package com.dataStucture.ElementaryDataStructures.array.SearchingSorting;

public class FindElementLinerSearch {

	public static void findElement(int length, Integer array[], int key) {
		for (int i = 0; i < length; i++) {
			int counter = 0;
			if (array[i] == key) {
				counter++;
				System.out.println(key + " :locaton " + array[i]);
			} else {
				System.out.println("match not found");
			}
		}
	}

	public static void main(String[] args) {
		Integer[] array = { 12, 78, 88, 55, 12, 11, 4, 55, 8 };
		Integer key = 8;
		Integer length = array.length;
		findElement(length, array, key);
	}
}
