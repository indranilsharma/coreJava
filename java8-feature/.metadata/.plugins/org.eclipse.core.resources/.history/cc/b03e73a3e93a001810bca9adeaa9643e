interface FuntinalInterfaceDemo {
	void sam();
}

public class FuntinalInterfaceSAM implements FuntinalInterfaceDemo {

	public static void main(String[] args) {
     
	}

	@Override
	public void sam() {
     System.out.println("SAM - Implemented");		
	}

}
