package com.operators.assignmentOperator;

/**
 *
 * @author root
 */
public class Manager4 {

    public static void main(String[] argv) {
        int i = 0;
        int b = 9;
        i = b;
        System.out.println(i);

        byte x = 2;
        x = (byte) (x + 3);
        System.out.println(x);
    }
}
