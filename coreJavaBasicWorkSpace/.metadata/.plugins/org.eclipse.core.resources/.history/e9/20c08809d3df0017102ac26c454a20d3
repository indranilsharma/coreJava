package com.operators.arithmaticOperators;

/**
 *
 * @author root
 */
public class Manager9 {
    public static void main(String[] argv){
        System.out.println(11 % 3 );//3
        System.out.println(11 % -3);//3
        System.out.println(-11 % 3);//-3
        System.out.println(-11 % -3);//
        System.out.println(11.5 % 3.2);
        System.out.println(11.5 % -3.2);
        System.out.println(-11.5 % 3.2);
        System.out.println(-11.5 % -3.2);
    }
}
