package com.classesMethodsVariable.Variable.staticVariable;
/**
 *
 * @author root
 */
public class Manager5 {

    static int i = 10;
    static int j;
    static int k ;
    static  int m;

    public static void main(String[] args) {
        int i = 20;
        System.out.println("i:" + i);
        System.out.println("i:" + Manager5.i);
        System.out.println("j:" + j);
        Manager5.j = 1000;
        System.out.println("Manager5.j :" + j);

        System.out.println(k);// i and i diffarent 
        k = 88;
        System.out.println(k);
        System.out.println(Manager5.k);
  
        System.out.println(m);// i and i diffarent 
        int m = 4488;//m and local m are diffrent 
        System.out.println(m);
        System.out.println(Manager5.m);
    }

}
