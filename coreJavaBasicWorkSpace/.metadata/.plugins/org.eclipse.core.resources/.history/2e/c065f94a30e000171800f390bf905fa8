package com.operators.unaryOparator;

/**
 *
 * @author root
 */
public class Manager1 {

    public static void main(String[] args) {
        int i = 0;
        int j=0;
        int k=i++;
        System.out.println(i++);
        System.out.println(i);//next uses
        System.out.println(j--);
        System.out.println(j);
        System.out.println(k);
    }
}
