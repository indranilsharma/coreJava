package com.operators.arithmaticOperators;

/**
 *
 * @author root
 */
public class Manager4 {
    public static void main(String[] argv){
        System.out.println(10 / 3);//3
        System.out.println(-10 / 3);//3
        System.out.println(10 / -3 );//3
        System.out.println(11 / 3 );//3
        
        System.out.println(-11 / 3);
        System.out.println(11 / -3);
    }
}
