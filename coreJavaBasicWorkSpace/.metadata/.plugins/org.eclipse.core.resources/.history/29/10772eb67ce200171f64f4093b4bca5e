package com.statement.loop;

public class Manager5 {
	public static void main(String[] argv){
	    /*int i = 7;
	    for (i++, int j = 0; i < 10; j++) { } */ // illegal!
	    for (int i = 7, j = 0; i < 10; j++) {
	    	System.out.println(i+j);
	    }
	    for (int k = 0; k < 10; k++) {
	        System.out.println(k);
	      }
	    }
}
