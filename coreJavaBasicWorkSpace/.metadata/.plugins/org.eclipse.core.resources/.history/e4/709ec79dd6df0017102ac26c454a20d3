package com.operators.assignmentOperator;

/**
 *
 * @author root
 */
public class Manager3 {
 public static void main(String args[]) {
  int i = 1;
  int j = 2;
  int k = 3;
  int l = 4;
  i = j = k = l;
  System.out.println(i);
  System.out.println(j);
  System.out.println(k);
  System.out.println(l);
 }   
}
