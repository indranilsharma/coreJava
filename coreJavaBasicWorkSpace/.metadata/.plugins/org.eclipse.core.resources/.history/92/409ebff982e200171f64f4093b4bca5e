package com.statement.switchCase;

public class Manager5 {
	public static void main(String[] argv) {
		switch (new Integer(4)) {
		case 4:
			System.out.println("boxing is OK");
		}
		
		switch (new String("A")) {
		
		case "A":
			System.out.println("A");
			break;
		case "B":
			System.out.println("B");
			break;

		default:
			System.out.println("D");
			break;
		}
	}
}
