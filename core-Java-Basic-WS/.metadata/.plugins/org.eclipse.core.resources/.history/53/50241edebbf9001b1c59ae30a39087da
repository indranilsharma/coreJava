package com.Thread.Thread_Method.basic;

class MyManager1Child extends Thread {

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println("child : " + i);
		}
	}
}

public class Manager1 {

	public static void main(String[] args) {

		MyManager1Child childThread = new MyManager1Child();
//		childThread.start();
		
//		if replace with start() with run() only one thread (main) will created 
//		run() will execute normal method    
		childThread.run();

		// can't expect the exact output
		for (int i = 0; i < 10; i++) {
			System.out.println("main thread :" + i);
		}
		System.out.println(".......");
	}
}
