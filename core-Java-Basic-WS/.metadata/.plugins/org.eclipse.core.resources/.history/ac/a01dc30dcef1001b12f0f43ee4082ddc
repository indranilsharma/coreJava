package com.CollectionAPI.CollectionFramework.set.Hashset;

import java.util.HashSet;
import java.util.Iterator;

public class Hashset_Demo {
	public static void main(String[] args) {

		HashSet<Object> hs = new HashSet<>();

		hs.add("India");
		hs.add("Australia");
		hs.add("South Africa");
		hs.add(null);
		hs.add(100);
		hs.add(true);
		System.out.println(hs);

		// it will return false for already present item
		System.out.println(hs.add("Australia"));
		System.out.println(hs);

		hs.remove(100);
		System.out.println(hs);

		Iterator<Object> itr = hs.iterator();

		while (itr.hasNext()) {
			Object next = itr.next();
			System.out.println(next);
		}
	}
}
