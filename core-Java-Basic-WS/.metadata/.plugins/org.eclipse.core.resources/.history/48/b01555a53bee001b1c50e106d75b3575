package com.CollectionAPI.CollectionFramework.list.linkList;

import java.util.LinkedList;

public class Manager1 {
	public static void main(String args[]) {

		LinkedList<Object> list = new LinkedList<>();
		
		list.add("A");
		list.add("B");
		list.addFirst("India");
		list.addLast(null);
		list.add(4, "CopyOn");
		
		System.out.println(list);
		
		list.remove(2);
		list.remove("B");
		list.removeLast();
		System.out.println(list);

		list.removeFirst();
		
		System.out.println(list);
		
		list.set(1, "nullReplace");
		System.out.println(list);

		//in foreach loop
		for (Object i : list) {
			System.out.println(list);
		}
		
		for (int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i));
		}
	}
}
